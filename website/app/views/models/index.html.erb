
<h1>Models</h1>

<table>
  <thead>
    <tr>
      <th>Good</th>
      <th>Id</th>
      <th>Author</th>
      <th>Date</th>
      <th>Body</th>
      <th>Team</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @models.each do |model| %>
      <tr>
        <td><%= model.good %></td>
        <td><%= model.id %></td>
        <td><%= model.author %></td>
        <td><%= model.date %></td>
        <td><%= model.body %></td>
        <td><%= model.team %></td>
        <td><%= link_to 'Show', model %></td>
        <td><%= link_to 'Edit', edit_model_path(model) %></td>
        <td><%= link_to 'Destroy', model, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Model', new_model_path %>
